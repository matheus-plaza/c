//1
#include <stdio.h>

int main()
{
    float precocusto, acress, valvenda;
    
    printf("Digite o preço de custo do produto: ");
    scanf("%f", &precocusto);
    
    printf("Digite o percentual de acressimo: ");
    scanf("%f", &acress);
    
    valvenda = precocusto + precocusto * (acress / 100);
    
    printf("O valor de venda do produto é de: %.2f", valvenda);

    return 0;
}

//2
#include <stdio.h>

int main()
{
    float  valdeposito, valtotal;
    
    printf("Digite o valor depositado: ");
    scanf("%f", &valdeposito);
    
    valtotal = valdeposito + valdeposito / 200;
    
    printf("O valor total é de: %.2f", valtotal);

    return 0;
}

//3
#include <stdio.h>

int main()
{
    float  A, B;
    
    printf("Digite o valor de a: ");
    scanf("%f", &A);
    
    printf("Digite o valor de b: ");
    scanf("%f", &B);
    
    printf("O valor total de a trocado é %.2f\n", B);
    printf("O valor total de b trocado é %.2f", A);

    return 0;
}

#include <stdio.h>
int main()
{

int vetor[6], i;
    
    for(i=0; i<6; i++){
        printf("digite o %dÂº valor: ", i + 1);
        scanf("%d", &vetor[i]);
    }
    
    printf("os valores de traz para frente ficam:\n");

    
    for(i=5; i>=0; i--){
        printf("%d\n", vetor[i]);
    }    
    
    return 0;

}

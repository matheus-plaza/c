/*1*/
#include <stdio.h>

int main()
{
    int i;
    
    for(i=50; i<=250; i++){
        printf("%d\n", i);
    }

    return 0;
}

/*2*/
#include <stdio.h>

int main()
{
    int i;
    
    for(i=3000; i>=100; i--){
        printf("%d\n", i);
    }

    return 0;
}

/*3*/
#include <stdio.h>

int main()
{
    int i, x;
    
    for(i=29; i>=1; i--){
        
        x = i + 1;
        
        if(x % 2 == 0){
            printf("%d\n", i);
        }
    }
    return 0;
}

/*4.*/
#include <stdio.h>

int main()
{
    int i, x;
    
    for(i=600; i<=1000; i++){
        
            printf("%d\n", i * i + 1);
    }
    return 0;
}

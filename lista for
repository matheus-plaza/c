/*1*/
#include <stdio.h>

int main()
{
    int i;
    
    for(i=50; i<=250; i++){
        printf("%d\n", i);
    }

    return 0;
}

/*2*/
#include <stdio.h>

int main()
{
    int i;
    
    for(i=3000; i>=100; i--){
        printf("%d\n", i);
    }

    return 0;
}

/*3*/
#include <stdio.h>

int main()
{
    int i, x;
    
    for(i=29; i>=1; i--){
        
        x = i + 1;
        
        if(x % 2 == 0){
            printf("%d\n", i);
        }
    }
    return 0;
}

//4

#include <stdio.h>

int main()
{
int i, soma=0;

for(i=600; i<=1000; i++){
    soma += i;
}
    printf("%d", soma);
    return 0;
}
    
    
/* 5.*/
#include <stdio.h>

int main()
{
   int quantidade, numero, soma=0;
   
   printf("digite a quantia de numeros desejada: ");
   scanf("%d", &quantidade);
   
   printf("digite os numeros:\n");
   
   for(int i = 0; i < quantidade; i++){
       scanf("%d", &numero);
       
       if(numero % 2 == 0 && numero > 5){
           soma += numero;
       }
       
    }
   
    printf(" = %d\n", soma);

    return 0;
}

/* 6.*/
#include <stdio.h>

int main()
{
    int quantia, numero, soma = 0;
    
    printf("digite a quantia que deseja somar: ");
    scanf("%d", &quantia);
    
    printf("digite os numeros:\n");
    
    for(int i=0; i<quantia; i++){
        scanf("%d", &numero);
        
        if(numero < 0){
            soma += numero;
        }
    }

    printf("soma = %d", soma);

    return 0;
}

    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    

#include <stdio.h>

int main()
{
    int aux = 0, num[10];
    
    printf("digite 10 numeros");
    
    for(int i=0;i<10;i++){
        scanf("%d", &num[i]);
    }
    
    for(int i=0;i<10;i++){
        for(int j=i;j<10;j++){
            
            if( num[i] > num[j]){
                aux    = num[i];
                num[i] = num[j];
                num[j] = aux;
            }
        }
    }
    
    printf("a ordem dos numeros Ã© \n");
    
    for(int i=0;i<10;i++){
        printf("%d ", num[i]);
    }

    return 0;
}
